<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/vehicle_container"
    tools:context=".AttendanceFrag.AttListFrag">


    <TextView
        android:id="@+id/att_date_select_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:background="@drawable/common_google_signin_btn_icon_dark_normal_background"
        android:paddingStart="16dp"
        android:paddingEnd="16dp"
        android:text="Date"
        android:textColor="@color/black"
        android:textSize="20sp" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/present_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/att_date_select_text"></androidx.recyclerview.widget.RecyclerView>

</RelativeLayout>